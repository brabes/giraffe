signature G_LIB_THREAD_PRIORITY =
  sig
    datatype t =
      LOW
    | NORMAL
    | HIGH
    | URGENT
    val null : t
    structure C :
      sig
        type val_
        type ref_
        val withVal :
          (val_ -> 'a)
           -> t
           -> 'a
        val withRefVal :
          (ref_ -> 'a)
           -> t
           -> (val_, 'a) pair
        val fromVal : val_ -> t
        exception Value of FFI.Enum.val_
      end
  end
